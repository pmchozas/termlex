  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The Source class represents the provenance information of an element, may it be the authorship, notes or other sources."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Source"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The TerminologicalConcept class represents a mental abstraction, concept or unit of thought that embodies the meaning of one or more lexical entries, that refer to the same lexical sense, thus, belong to the same area of epertise."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Terminological Concept"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  }, {
    "@id" : "http://www.w3.org/2004/02/skos/core#Concept"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#Usage",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The Usage class represents usage notes, advices or indications regarding the lexical sense of a given terminological concept. "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Usage"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "http://www.w3.org/2002/07/owl#Thing"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#concept",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The concept property relates an ontological entity to a terminological concept that represents the corresponding meaning."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "concept"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#isConceptOf"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#evokes",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The evokes property relates a lexical entry to the terminological concepts it evokes."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.w3.org/ns/lemon/ontolex#LexicalEntry"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "evokes"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#isEvokedBy"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#isConceptOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The isConceptOf property relates the meaning of a Terminological Concept by linking it to a particular ontological meaning."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "is concept of"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#isEvokedBy",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The isEvokedBy property relates a terminological concept to one of the lexical entries that evoke it."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "is evoked by"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.w3.org/ns/lemon/ontolex#LexicalEntry"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#isLexicalizedSenseOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#FunctionalProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The isLexicalizedSense property relates a lexical sense to a unique corresponding terminological concept."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "http://www.w3.org/ns/lemon/ontolex#LexicalSense"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "is lexicalized sense of"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#lexicalizedSense",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty", "http://www.w3.org/2002/07/owl#FunctionalProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The lexicalizedSense property relates a terminological concept to a unique corresponding lexical sense."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#TerminologicalConcept"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "lexicalized sense"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "http://www.w3.org/ns/lemon/ontolex#LexicalSense"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#isLexicalizedSenseOf"
  } ]
}, {
  "@id" : "https://termlex.oeg.fi.upm.es/termlex#reliabilityCode",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The reliabilityCode property relates a lexical sense to the the reliability code class."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Reliability Code"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://termlex.oeg.fi.upm.es/termlex#ReliabilityCode"
  } ]
